#: actions.cpp:237
msgid "File \"{}\" on line {}: {}"
msgstr "ფაილი \"{}\" ხაზზე {}: {}"

#: actions.cpp:636 actions.cpp:642 actions.cpp:648 actions.cpp:654
msgid "\"HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD\" format expected"
msgstr "მოველოდი ფორმატს \"HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD\""

#: actions.cpp:676
msgid "Unknown \"enabled\" option value \"{}\""
msgstr "უცნობი \"enabled\" პარამეტრის მნიშვნელობა \"{}\""

#: actions.cpp:683
msgid "Unknown option \"{}\""
msgstr "უცნობი პარამეტრი \"{}\""

#: actions.cpp:711
msgid "Unknown hook \"{}\""
msgstr "უცნობი ჰუკი \"{}\""

#: actions.cpp:720
msgid ""
"Package filter can only be used in GOAL_RESOLVED, PRE_TRANS and POST_TRANS "
"hooks"
msgstr ""
"პაკეტების ფილტრი, მხოლოდ, GOAL_RESOLVED, PRE_TRANS და POST_TRANS ჰუკებში "
"შეგიძლიათ, გამოიყენოთ"

#: actions.cpp:726
msgid "Cannot use direction without package filter"
msgstr "მიმართულებას პაკეტების ფილტრის გარეშე ვერ გამოიყენებთ"

#: actions.cpp:740
msgid "Unknown package direction \"{}\""
msgstr "უცნობი პაკეტის მიმართულება \"{}\""

#: actions.cpp:747
msgid "Unknown mode \"{}\""
msgstr "უცნობი რეჟიმი \"{}\""

#: actions.cpp:755
msgid "Unsupported value of the \"raise_error\" option: {}"
msgstr "მხარდაუჭერელი მნიშნელობა პარამეტრისთვის \"raise_error\": {}"

#: actions.cpp:760
msgid "Missing command"
msgstr "ნაკლული ბრძანება"

#: actions.cpp:953
msgid "Synax error: Missing equal sign (=) in action output line: {}"
msgstr ""
"სინტაქსის შეცდომა: აკლია ტოლობის ნიშანი(=) ქმედების გამოტანის ხაზზე: {}"

#: actions.cpp:964
msgid "Cannot set option: Action output line: {}"
msgstr "პარამეტრის დაყენება შეუძლებელია: ქმედების გამოტანილი ხაზი: {}"

#: actions.cpp:982
msgid "Action sent the wrong log level: {}"
msgstr "ქმედებამ გააგზავნა არასწორი ჟურნალის დონე: {}"

#: actions.cpp:987 actions.cpp:1794
msgid "Action calls for stop: {}"
msgstr "ქმედების გამოძახებები გასაჩერებლად: {}"

#: actions.cpp:990 actions.cpp:1801
msgid "Action sent error message: {}"
msgstr "ქმედების გამოგზავნილი შეცდომის შეტყობინება: {}"

#: actions.cpp:995
msgid ""
"Syntax error: Action output line must start with \"tmp.\" or \"conf.\" or "
"\"var.\" or \"stop=\" or \"error=\": {}"
msgstr ""
"სინტაქსის შეცდომა: ქმედების გამოტანის ხაზი უნდა იწყებოდეს ერთ-ერთი "
"სტრიქონით: \"tmp.\", \"conf.\", \"var.\", \"stop=\", ან \"error=\": {}"

#: actions.cpp:1020
msgid "Error reading from action (from pipe)"
msgstr "ქმედებიდან (გადაცემის არხიდან) წაკითხვის შეცდომა"

#: actions.cpp:1038
msgid "Syntax error in json request from action: Missing starting '{{' char"
msgstr ""
"სინტაქსის შეცდომა json ტიპის მოთხოვნაში ქმედებიდან: აკლია დამწყები '{{' "
"სიმბოლო"

#: actions.cpp:1062
msgid "Error during processing of a request from action."
msgstr "შეცდომა ქმედებიდან მოთხოვნის დამუშავებისას."

#: actions.cpp:1071
msgid "Syntax error in json request from action: {}"
msgstr "სინტაქისს შეცდომა json მოთხოვნაში ქმედებიდან: {}"

#: actions.cpp:1079
msgid "Syntax error in json request from action: Incomplete input"
msgstr "სინტაქსის შეცდომა json მოთხოვნაში ქმედებიდან: შეყვანა დაუსრულებელია"

#: actions.cpp:1154
msgid "Cannot write response"
msgstr "პასუხის ჩაწერა შეუძლებელია"

#: actions.cpp:1821
msgid "Actions plugin: Cannot create pipe"
msgstr "ქმედების დამატება: ვერ შევქმენი ფაიფი"

#: actions.cpp:1913
msgid "Actions plugin: Cannot fork"
msgstr "ქმედების დამატება: დაფორკვა შეუძლებელია"

#: actions.cpp:1968
msgid "Actions plugin: Cannot bind command stdin"
msgstr ""
"ქმედების დამატება: ბრძანების სტანდარტულ შეყვანაზე მიმაგრება შეუძლებელია"

#: actions.cpp:1970
msgid "Actions plugin: Cannot bind command stdout"
msgstr "ქმედების დამატება: ბრძანების გამოტანის არხზე მიმაგრება შეუძლებელია"

#: actions.cpp:1988
msgid "Cannot execute action, command \"{}\" arguments \"{}\""
msgstr "ქმედების შესრულება შეუძლებელია. ბრძანება \"{}\". არგუმენტები \"{}\""

#: actions.cpp:1996
msgid "Error during preparation child process"
msgstr "შეცდომა შვილი პროცესის მომზადებისას"

#: actions.cpp:2015
msgid "Exit code: {}"
msgstr "გასვლის კოდი: {}"

#: actions.cpp:2019
msgid "Terminated by signal: {}"
msgstr "შეწყდა სიგნალით: {}"

