#: actions.cpp:397 actions.cpp:405 actions.cpp:413 actions.cpp:421
msgid ""
"Error in file \"{}\" on line {}: \"HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD\" "
"format expected"
msgstr ""
"Erro no arquivo \"{}\" na linha {}: Esperava-se o formato \"GANCHO:"
"FILTRO_PACOTE:DIREÇÃO:OPÇÕES:COMANDO\""

#: actions.cpp:443
msgid ""
"Error in file \"{}\" on line {}: Unknown \"enabled\" option value \"{}\""
msgstr ""
"Erro no arquivo \"{}\" na linha {}: Opção \"enabled\" com valor desconhecido "
"\"{}\""

#: actions.cpp:450
msgid "Error in file \"{}\" on line {}: Unknown option \"{}\""
msgstr "Erro no arquivo \"{}\" na linha {}: Opção desconhecida \"{}\""

#: actions.cpp:468
msgid "Error in file \"{}\" on line {}: Unknown hook \"{}\""
msgstr "Erro no arquivo \"{}\" na linha {}: Gancho desconhecido \"{}\""

#: actions.cpp:478
msgid ""
"Error in file \"{}\" on line {}: Package filter can only be used in "
"PRE_TRANS and POST_TRANS hooks"
msgstr ""
"Erro no arquivo \"{}\" na linha {}: O filtro de pacotes só pode ser usado em "
"ganchos PRE_TRANS e POST_TRANS"

#: actions.cpp:488
msgid ""
"Error in file \"{}\" on line {}: Cannot use direction without package filter"
msgstr ""
"Erro no arquivo \"{}\" na linha {}: Não é possível usar direção sem filtro "
"de pacotes"

#: actions.cpp:505
msgid "Error in file \"{}\" on line {}: Unknown package direction \"{}\""
msgstr ""
"Erro no arquivo \"{}\" na linha {}: Pacote com direção desconhecida \"{}\""

#: actions.cpp:514
msgid "Error in file \"{}\" on line {}: Missing command"
msgstr "Erro no arquivo \"{}\" na linha {}: Faltando comando"
