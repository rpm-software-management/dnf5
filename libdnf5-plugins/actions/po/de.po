#: actions.cpp:237
msgid "File \"{}\" on line {}: {}"
msgstr "Datei „{}“ in Zeile {}: {}"

#: actions.cpp:636 actions.cpp:642 actions.cpp:648 actions.cpp:654
msgid "\"HOOK:PKG_FILTER:DIRECTION:OPTIONS:CMD\" format expected"
msgstr "„HOOK:PAKETFILTER:RICHTUNG:OPTIONEN:BEFEHL“-Format erwartet"

#: actions.cpp:676
msgid "Unknown \"enabled\" option value \"{}\""
msgstr "Unbekannter Wert „{}“ der Option „enabled“"

#: actions.cpp:683
msgid "Unknown option \"{}\""
msgstr "Unbekannte Option „{}“"

#: actions.cpp:711
msgid "Unknown hook \"{}\""
msgstr "Unbekannte Hook „{}“"

#: actions.cpp:720
msgid ""
"Package filter can only be used in GOAL_RESOLVED, PRE_TRANS and POST_TRANS "
"hooks"
msgstr ""
"Paketfilter kann nur in den Hooks GOAL_RESOLVED, PRE_TRANS und POST_TRANS "
"genutzt werden"

#: actions.cpp:726
msgid "Cannot use direction without package filter"
msgstr "Richtung kann nicht ohne Paketfilter genutzt werden"

#: actions.cpp:740
msgid "Unknown package direction \"{}\""
msgstr "Unbekannte Paketrichtung „{}“"

#: actions.cpp:747
msgid "Unknown mode \"{}\""
msgstr "Unbekannter Modus „{}“"

#: actions.cpp:755
msgid "Unsupported value of the \"raise_error\" option: {}"
msgstr "Nicht unterstützter Wert der Option „raise_error“: {}"

#: actions.cpp:760
msgid "Missing command"
msgstr "Befehl fehlt"

#: actions.cpp:953
msgid "Synax error: Missing equal sign (=) in action output line: {}"
msgstr ""
"Syntaxfehler: Gleichheitszeichen (=) in Ausgabezeile der Aktion fehlt: {}"

#: actions.cpp:964
msgid "Cannot set option: Action output line: {}"
msgstr "Option kann nicht gesetzt werden: Ausgabezeile der Aktion: {}"

#: actions.cpp:982
msgid "Action sent the wrong log level: {}"
msgstr "Aktion hat die falsche Protokollierstufe gesendet: {}"

#: actions.cpp:987 actions.cpp:1794
msgid "Action calls for stop: {}"
msgstr "Aktion fordert Stopp: {}"

#: actions.cpp:990 actions.cpp:1801
msgid "Action sent error message: {}"
msgstr "Aktion hat eine Fehlermeldung gesendet: {}"

#: actions.cpp:995
msgid ""
"Syntax error: Action output line must start with \"tmp.\" or \"conf.\" or "
"\"var.\" or \"stop=\" or \"error=\": {}"
msgstr ""
"Syntaxfehler: Die Aktionsausgabzeile muss mit „tmp.” oder „conf.” oder „var.”"
" oder „stop=” oder „error=” beginnen: {}"

#: actions.cpp:1020
msgid "Error reading from action (from pipe)"
msgstr "Fehler beim Lesen aus der Aktion (aus der Pipe)"

#: actions.cpp:1038
msgid "Syntax error in json request from action: Missing starting '{{' char"
msgstr ""
"Syntaxfehler in JSON-Anfrage von der Aktion: Zeichen „{{“ am Anfang fehlt"

#: actions.cpp:1062
msgid "Error during processing of a request from action."
msgstr "Fehler bei der Verarbeitung einer Anfrage von der Aktion."

#: actions.cpp:1071
msgid "Syntax error in json request from action: {}"
msgstr "Syntaxfehler in der JSON-Anfrage der Aktion: {}"

#: actions.cpp:1079
msgid "Syntax error in json request from action: Incomplete input"
msgstr "Syntaxfehler in der JSON-Anfrage der Aktion: Eingabe ist unvollständig"

#: actions.cpp:1154
msgid "Cannot write response"
msgstr "Antwort kann nicht geschrieben werden"

#: actions.cpp:1821
msgid "Actions plugin: Cannot create pipe"
msgstr "Aktionen-Plugin: Pipe kann nicht erstellt werden"

#: actions.cpp:1913
msgid "Actions plugin: Cannot fork"
msgstr "Aktionen-Plugin: Forken nicht möglich"

#: actions.cpp:1968
msgid "Actions plugin: Cannot bind command stdin"
msgstr "Aktionen-Plugin: Befehls-Standardeingabe kann nicht gebunden werden"

#: actions.cpp:1970
msgid "Actions plugin: Cannot bind command stdout"
msgstr "Aktionen-Plugin: Befehls-Standardausgabe kann nicht gebunden werden"

#: actions.cpp:1988
msgid "Cannot execute action, command \"{}\" arguments \"{}\""
msgstr "Aktion kann nicht ausgeführt werden, Befehl „{}“, Argumente „{}“"

#: actions.cpp:1996
msgid "Error during preparation child process"
msgstr "Fehler während der Vorbereitung des Kindprozesses"

#: actions.cpp:2015
msgid "Exit code: {}"
msgstr "Exit-Code: {}"

#: actions.cpp:2019
msgid "Terminated by signal: {}"
msgstr "Beendet durch Signal: {}"

