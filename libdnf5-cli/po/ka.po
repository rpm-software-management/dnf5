#: argument_parser.cpp:480
msgid "Command id \"{}\" does not contain named argument with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" მითითებულ არგუმენტს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:488
msgid "Command id \"{}\" does not contain positional argument with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" პოზიციურ არგუმენტს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:495
msgid "Command id \"{}\" does not contain group with id \"{}\""
msgstr "ბრძანება ID-ით \"{}\" ჯგუფს ID-ით \"{}\" არ შეიცავს"

#: argument_parser.cpp:590 argument_parser.cpp:718
msgid "Unknown argument \"{}\" for command \"{}\""
