#: argument_parser.cpp:84
msgid "Invalid character '.' in argument id \"{}\""
msgstr "Недопустимый символ '.' в идентификаторе аргумента \"{}\""

#: argument_parser.cpp:199 argument_parser.cpp:204
msgid "Too few values for positional argument \"{}\""
