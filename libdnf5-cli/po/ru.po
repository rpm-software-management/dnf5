#: argument_parser.cpp:244
msgid "Missing value for named argument \"--{}\""
msgstr "Недостающее значение для именованного аргумента \"--{}\""

#: argument_parser.cpp:252
msgid "Unexpected value for named argumend \"--{}\""
msgstr "Неожиданное значение для именованного аргумента \"--{}\""

